{
  "name": "tweet-app",
  "productName": "Tweet",
  "version": "0.0.2",
  "description": "Desktop app only for tweeting. Timeline is never shown",
  "main": "main/index.js",
  "bin": {
    "tweet": "./bin/cli.js"
  },
  "scripts": {
    "build:tsc": "tsc -p .",
    "build:parcel": "parcel build -o preload.js -d renderer/ --target electron renderer/index.js",
    "build": "npm-run-all build:tsc build:parcel",
    "watch:tsc": "tsc -p . --watch",
    "watch:parcel": "parcel -o preload.js -d renderer/ --target electron renderer/index.js",
    "watch": "npm-run-all -p watch:tsc watch:parcel",
    "cli": "./bin/cli.js",
    "app": "NODE_ENV=development ./bin/cli.js",
    "debug": "TWEET_APP_DEBUG=true ./bin/cli.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rhysd/tweet-app.git"
  },
  "author": "rhysd <https://rhysd.github.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rhysd/tweet-app/issues"
  },
  "homepage": "https://github.com/rhysd/tweet-app#readme",
  "devDependencies": {
    "@types/electron-window-state": "^2.0.33",
    "@types/node": "^10.12.23",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.11.0",
    "typescript": "^3.3.3"
  },
  "optionalDependencies": {
    "electron": "^4.0.4"
  },
  "dependencies": {
    "electron-context-menu": "^0.11.0",
    "electron-window-state": "^5.0.3"
  }
}
